<h1>Characters#show</h1>
<p>Find me in app/views/characters/show.html.erb</p>

<%= @character.nickname %><br/>
<%= @character.character_class %><br/>
<%= @character.lvl %><br/>
<% if @character.user_id == current_user.id %>
<%= link_to "Edit", edit_character_path(@character.id) %><br/>
<%= link_to "Delete", character_path(@character.id), method: :delete %><br/>
<% end %>
<%= link_to "Back", root_path%><br/>