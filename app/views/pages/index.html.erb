<h1>Pages#index</h1>
<p>Find me in app/views/pages/index.html.erb</p>
<% if user_signed_in?%>
	<% if @characters == [] %>
		<p>No characters</p>
		<%= link_to  "Create new character", new_character_path %> <br>
		<%=  link_to  "Log out", destroy_user_session_path, method: :delete %><br/>
	<% else %>
		<%= link_to  "Create new character", new_character_path %>
		<hr>
		<% @character.each do | char | %> 
		<strong>nickname</strong><br>
		<%= char.nickname %><br/>
		<strong>class</strong><br>
		<%= char.character_class %><br/>
		<strong>lvl</strong><br>
		<%= char.lvl %><br/>
		<hr>
		<% end %>
		<%=  link_to  "Log out", destroy_user_session_path, method: :delete %><br/>
		<%=  link_to  "Create item", new_item_path%><br/>
		
	<% end %>
<% else %>
	<%=  link_to  "Log in", new_user_session_path %><br>
<%end%>
